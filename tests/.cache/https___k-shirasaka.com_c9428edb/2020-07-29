    asNavFor: '#sub_slider'
  });
  $('#sub_slider').slick({
    vertical: true,
    focusOnSelect: true,
    infinite: true,
    dots: false,
    arrows: false,
    slidesToShow: 4,
    slidesToScroll: 1,
    autoplay: false,
    asNavFor: '#main_slider'
  });
});

</script>

