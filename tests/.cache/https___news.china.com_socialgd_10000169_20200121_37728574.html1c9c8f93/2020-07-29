<script>
/**
* éè¿data-gridå±æ§è·åæµ®å¨å¼å§ä½ç½®
*  @param {type} id ä»idåç´ ä½ç½®å¼å§æµ®å¨
*  @param {int} ä¼ å¥æ»å¨æ°å¼
*  @param {type} end æ»å¨å°idåç´ ä½ç½®åæ­¢æµ®å¨
*/
function itemPosition(start,scroll,end){
  var id = $('#'+start),
      end = end||'',
      endgrid = 0;
  if(id.length>0){
      if($('#'+end).length>0){
          var endobj = $('#'+end);
          endgrid = endobj.offset().top;
      }
      //console.log(endgrid);
      var figure = id.attr('data-grid') || id.offset().top;
      if(scroll>figure){
          if(endgrid && scroll>endgrid){
              id.removeClass("fixed");
          }else{
              id.addClass("fixed");
          }
      }else{
          id.removeClass("fixed");
      }
  }
};

/**
* æ´ä½å½æ°è°ç¨ç¸å³
*/
function init(){
// çªå£ä¸ä¸æ»å¨ç¸å³
    $(window).scroll(function() {
        var scltop = $(window).scrollTop();
        // å¯¼èªæµ®å¨
        itemPosition('js-article-title',scltop,'js-article-bottom');//ç»æé¡µ
        });
}
$(function(){
  init();
});
</script>
