<script type="text/javascript">

        jQuery(document).ready(function () {
            var sessionRefresherSeconds = 0;
            var lastSessionRefreshAnswer = "-";
            var secondsToWait = 30;   // first check after 1/2 Minute
            function sessionRefresh()     // is called every second
            {
                // Countdown
                sessionRefresherSeconds++;
                if (sessionRefresherSeconds > secondsToWait) {
                    sessionRefresherSeconds = 0;
                    url = "/session/SessionHolderAnswer/?rnd=" + new Date().getTime();
                    jQuery.ajax({
                        type: "GET",
                        url: url,
                        success: function (data) {
                            lastSessionRefreshAnswer = data;
                            if (data.gesperrt === true) {
                                window.location = data.redirectTo;
                            }
                            secondsToWait = 1 * 60;  // now check every 1 minute
                        },
                        error: function (xhr, ajaxOptions, thrownError) {
                            lastSessionRefreshAnswer = "Error: " + thrownError;
                            console.error('sessionRefresh:' + lastSessionRefreshAnswer);
                        }
                    });
                }
            }
            window.setInterval(sessionRefresh, 1000);
        });

</script>


<div id="cookieInfo" class="privacy-warning permisive" style="display: none !important;">
    <div class="info">
        <div class="cd-message">
            <h3>Diese Website verwendet ausschließlich funktionale Cookies,</h3>
            <p>
                die notwendig sind, um den vollen Funktionsumfang unseres Datenbank-Angebotes sicherzustellen.
                <br /> <a href="http://go.nwb.de/datenschutz">Lesen Sie auch unsere Datenschutzerklärung!</a>
            </p>
        </div>
