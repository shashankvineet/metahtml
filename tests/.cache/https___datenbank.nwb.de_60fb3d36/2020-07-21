    <div class="submit" id="cookieOk">
        <a href="#">Cookies akzeptieren</a>
    </div>
</div>

<script type="text/javascript">

    jQuery(document).ready(function () {
        if (getCookie('cookieVerstanden') !== 'ok') {
            jQuery("#cookieInfo").show();
        } else {
            jQuery("#cookieInfo").remove();
        }
    });

    jQuery("#cookieOk").click(function (evt) {
        evt.preventDefault();
        setCookie('cookieVerstanden', 'ok', 365);
        jQuery("#cookieInfo").remove();
    });

    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires=" + d.toUTCString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }

    function getCookie(cname) {
        var name = cname + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }
</script>


