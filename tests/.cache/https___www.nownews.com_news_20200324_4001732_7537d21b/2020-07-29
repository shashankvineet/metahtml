        // adArray.push('/5799246/Nownews_mob_ParallaxScrolling_1x1')
        // adArray.push('/5799246/NOWnews_ad_300x250_R')
        // if(window.location.href.includes("politics")){
        //     adArray.push('vd896755030')
        // }else if(window.location.href.includes("entertainment")){
        //     adArray.push('vd453312779')
        // }else{
        //     adArray.push('truvid')
        // }
        // //全局通用盖板
        // adArray.push('/5799246/Mobile_homeinter_300x600')
        // 获取所有的 val
        $($('div[val]').each(function(index,item){
            adArray.push($(item).attr('val').split(",")[0].replace(/'/g,""))
        }))
        // 获取所有的 adkey
        $('a[adkey]').each(function(i,item){
            adArray.push($(item).attr('adkey'))
        })
        adReport(Array.from(new Set(adArray)).join())
        if($('#replaceMedia')) {
            $('#replaceMedia').before("<div style='clear:both'></div>")
        }

        $('div[id*=replaceMedia]').each(function(index, media) {
            if($(media).children("blockquote").length == 1){
                $(media).after($(media).children())
                $(media).remove()
            }
        })
            // var popAdTimer=null
