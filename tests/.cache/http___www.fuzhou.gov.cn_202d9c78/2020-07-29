<script>
    require(["tools","jquery","list"],function( tools , $ , listModel ){
		
        var Ser = avalon.define({
            $id:"ser",
            list:[],
            currdata:'',
            $toggle:function(){
                $("#search_list").toggle();
            },
            sub1:function(){
                form.method="post"
                form.action="/smartSearch/main/search.xhtml?siteId=402849946077df37016077eea95e002f&token="+Math.random();
                form.submit();
            },
            sub2:function(){
                form.method="get"
                form.action="http://fz.zwfw.fujian.gov.cn/nloginIn?contType=3";
                form.submit();
            },
            getData:function(data){
                $("#search").val(data);
                $(".key").css("display","none");
            }
        })
        var currentindex = -1;
        var size = 0;
        function movethis(up) {
            currentindex = currentindex + (up ? -1 : 1);
            if (currentindex == size) {
                currentindex = 0;
            }
            else if (currentindex < 0) {
                currentindex = size-1 ;
            }
            $(".key li").removeClass("keylist");
            $($(".key li")[currentindex]).addClass("keylist");
            var textvalue = $($(".key li")[currentindex]).text();
            if(currentindex>3){
                $('.key').animate({'scrollTop' : "+=50px"},100);
            }else if(up=true&&currentindex<9){
                $('.key').animate({'scrollTop' : "-=50px"},100);
            }
            Ser.currdata = textvalue
        }
        $('#search').bind('keyup',function(e){
            if (e.keyCode == 38) {
                movethis(true);
            }else if (e.keyCode == 40) {
                movethis(false);
            }else if(e.keyCode == 13){
                if($("#search").val()==''){
                    Ser.sub1()
                }else if($("#search").val()!=''&&Ser.currdata!=''){
                    $("#search").val(Ser.currdata);
                    Ser.sub1()
                }else{
                    Ser.sub1()
                }
            }
         });
        $('#search').bind('keyup',function(){
            $(".key").css("display","block");
            var jqueryInput = $(this);
            var searchText = jqueryInput.val();
            $.ajax({
                url:"/smartSearch/interface/word/list.do?siteId=402849946077df37016077eea95e002f&pagesize=10&keyWord="+searchText,
                type:"get",
                dataType:"json",
                success:function(data){
                    Ser.list = data.rows;
                    if(Ser.list.length==0){
                        $(".key").css("display","none");
                    }
                    size = Ser.list.length
                },
                error:function(e){}
            })
         });
        $("#in_txt").on("input propertychange",function(){
            var txt = $(this).val();
            txt = txt.replace(/[,;'"]/,"").replace(/(alert|script|frame|src|href|document|eval|<\/|\/>|update|delete|select|%3B|img|<%|%>)/gi,"");
            var cLen =40;
            if(40-parseInt(txt.length)<=0){
                alert("åªè½è¾å¥1-40ä½çæ±å­ãå­ç¬¦ï¼");
                txt = txt.substring(0,40);
                $("#in_txt").val(txt);
            }
        })

        function lefScroll(){
            $(".leftclose").click(function(){
                $(".left_aside").hide();
            });
        }
        lefScroll();
        function lefScroll1(){
		    $(".leftclose1").click(function(){
		        $(".left_aside1").hide();
		    });
		}
        lefScroll1();
        function lefScroll2(){
            $(".leftclose2").click(function(){
                $(".left_aside2").hide();
            });
        }
		lefScroll2();

        avalon.define({
            $id:"zw_gd",
            zw_more:function(){
               $("#zw_list").toggle();
            }

        })
        tools.tab([
            {name : "Gzdt", index: 0},
            {name : "Szfwj", index: 0},
            {name : "Myzj", index: 0},
            {name : "Xglj", index : 0},
            {name : "Wxwb", index : 0},
            {name : "Ss", index : 0},
            {name:"zwfw",index:0},
            {name:"wyb",index:0},
            {name:"grbs",index:0},
            {name:"frbs",index:0},
            {name:"bmbs",index:0}
        ]);
        $(".focusBox").slide({
            titCell:".num",
            mainCell:".pic",
            effect:"fold",
            autoPlay:true,
            trigger:"mouseover",
            interTime:3500,
            autoPage:" <li><a></a><span></span></li>",
            startFun:function(i){
                jQuery(".focusBox .txt li").eq(i).animate({"bottom":0}).siblings().animate({"bottom":-36});
            }
        });

        $(".focusBoxrdzt").slide({
            titCell:".num1",
