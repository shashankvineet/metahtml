<script>
	require(["jquery"], function(jquery) {
		
		var dblogin = avalon.define({
			$id: "Login",
			isLogin: false, //å¤æ­æ¯å¦ç»å½
			username: '',
			isfav: false, //å¤æ­æ¯å¦æ¶è,é»è®¤æªæ¶è
			addFav: function(title, url) { //æ¶è
				if (!dblogin.isLogin) {
					var mymessage = confirm("è¯·åè¿è¡ç»å½ï¼");
					if (mymessage == true) { //ç¹å»ç¡®å®ï¼è·³è½¬ç»å½é¡µ
						window.open(
							"http://www.fuzhou.gov.cn/uc/userCenter/user/toLogin.htm?siteId=9137D6F29E2E6978E055000000000001");
					} else if (mymessage == false) {

					}
				}

				if (!dblogin.isfav && dblogin.isLogin) {
					jquery.ajax({
						url: "/uc/outer/collectionApp/addCollection.do",
						dataType: "json",
						contentType: 'application/json',
						type: "post",
						data: JSON.stringify({
							title: PageInfo.sitename,
							url: window.location.href,
							// random: Math.random()
						}),
						//jsonpCallback:"jsonpCallback",
						success: function(data) {
							//avalon.log(data);
							if (!data.error) {
								alert("æ¶èæåï¼")
								dblogin.isfav = true;
							}
						},
						error: function(data) {
							avalon.log("è¯·æ±å¤±è´¥ï¼");
						}
					});
				}
			},
			canceladdFav: function() { //åæ¶æ¶è

				var mymessage = confirm("ç¡®å®æ¯å¦è¦åæ¶æ¶èï¼");
				if (mymessage == true) { //ç¹å»ç¡®å®ï¼åæ¶æ¶è
					jquery.ajax({
						url: "/uc/outer/collectionApp/cancelCollection.do",
						dataType: "json",
						contentType: 'application/json',
						type: "post",
						data: JSON.stringify({
							title: PageInfo.sitename,
						}),
						//jsonpCallback:"jsonpCallback",
						success: function(data) {
							//avalon.log(data);
							if (!data.error) {
								alert("åæ¶æ¶èæåï¼")
								dblogin.isfav = false;
							}
						},
						error: function(data) {
							avalon.log("è¯·æ±å¤±è´¥ï¼");
						}
					});
				} else if (mymessage == false) {

				}

			}
		});

		var cspucToken = jquery.cookie("token");

		// avalon.log("==="+cookie);
		if (cspucToken) {

			jquery.ajax({
				url: "/uc/outer/user/getLoginUserName.do",
				dataType: "jsonp",
				type: "post",
				jsonpCallback: "jsonpCallback",
				success: function(data) {
					if (!data.error) { //ç»å½
						dblogin.isLogin = true;
						dblogin.username = data.userName;
					} else {
						dblogin.isLogin = false;
					}
				},
				error: function(data) {
					avalon.log("è¯·æ±å¤±è´¥ï¼");
				}
			});



			setTimeout(function(){
				//å¤æ­æ¯å¦æ¶è
				if (!dblogin.isLogin) {
					//avalon.log("æªç»å½ç¶æ")
				} else {
					//avalon.log("ç»å½ç¶æ")
					jquery.ajax({
						url: "/uc/outer/collectionApp/selCollection.do",
						dataType: "json",
						contentType: 'application/json',
						type: "post",
						data: JSON.stringify({
							title: PageInfo.sitename,
							url: window.location.href,
							// random: Math.random()
	
						}),
