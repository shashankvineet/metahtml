	<script language="javascript">

	function getMetaContent(propName) {
	    var metas = document.getElementsByTagName('meta');

	    for (i = 0; i < metas.length; i++) {
	        if (metas[i].getAttribute("name") == propName) {
	            return metas[i].getAttribute("content");
	        }
	    }

	    return "";
	} 

		var ct = getMetaContent("DCSext.ContentType"); 
		var cc = getMetaContent("DCSext.ContentChannel");  
		var str = getMetaContent("DCSext.DartZone"); 
		var n = str.lastIndexOf('/');
		var oc = str.substring(n + 1);
		
		<!-- SZM VERSION="2.0" -->
	
		if (str  == "qa.reuters.de/home" || str  == "reuters.de/home"){			
			var iam_data =
				 { 
					"st": "reuterde",  // Yes with the spelling mistake :)
					"cp": ct+"/"+cc,  //Our channel names for each page
					"sv": "ke",
					"mg": "yes",
					"oc" : oc 
					};			
		}
		
		else{
			var iam_data =
