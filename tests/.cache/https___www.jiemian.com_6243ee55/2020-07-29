    $(function() {
        var finished = true;
    	var page = 2;
		var lasttime = '1596020467';
    	var url = "//a.jiemian.com/index.php?m=index&a=indexAjax";
    	$(window).scroll(function() {
    		if (($(document).height() - $(window).height() - $(document).scrollTop()) <= $(window).height()+800 && finished)
    		{
        		finished = false;
        		$.get(url.replace('http://', '//'), {page : page,lasttime:lasttime}, function(data) {
            		var len = data.length - 1;
            		$.each(data, function(i, n) {
                		if ( typeof(n.type) != 'undefined' && n.type == 'before' ) {
                    		$('#'+n.id).before(n.rst);
                    	} else {
	                		$('#'+n.id).append(n.rst);
                		}
                		$("img[data-lazy-img]").lazyload();
                		if ( i == len && page < 4 ) {
                			page++;
                			finished = true;
                        } else if (page == 4) {
							getJMediaList(1, true);
						}
                	});
            	}, 'jsonp');
    		}
    	});
		$('#index-right-ad1').each(function() {
			var _this = this;
			var st = setTimeout(function() {
				var scriptStr = $(_this).find('script').html();
				if ( scriptStr != 'undefined' ) {
					scriptStr = scriptStr.replace('document.write', '$(#index-right-ad1).html');
					eval(scriptStr);
					clearTimeout(st);
				}
			}, 100);
		});
        if($(".blxxjb").length > 0) {
            $('.blxxjb').bxSlider({
                auto: true,
                mode: 'vertical',
                speed: 500,
                slideWidth: 320,
                pagerAutoWidth: true,
                autoHover: true,
                pause: 4000,
                pager: false,
                controls: false
            });
        }

		

		// 图片预加载
		function showImg(index) {
			var obj = $('#jmedia').find('ul li:eq(' + index + ') a');
			var imglen = obj.length;
			for (var i = 0; i < imglen; i++) {
				obj.eq(i).find('img').attr('src', obj.eq(i).find('img').attr('data-img'));
				obj.eq(i).find('img').removeAttr('data-img');
			}
		}

		$('.media-content').delegate('.bx-pager-item', 'click', function () {
			var self = $(this),
				index = self.find('a').attr('data-slide-index');

			getJMediaList(index * 1 + 1);
		});

		function getJMediaList(page, flag) {
			$.ajax({
				type: "post",
				url: "https://a.jiemian.com/index.php?m=index&a=indexAjaxJmedia&page=" + page,
				dataType: "jsonp",//数据类型为jsonp  
				jsonp: "callback",
				success: function (res) {
					var d = res[0],
						lis = '';
					
					if (page == 1 && flag) {
						for (var i = 0; i < d.pageCount; i++) {
							lis += '<li slide-index="'+ (i + 1) +'"></li>';
						}
						$('.media-list').append(lis);

						$('.media-list').bxSlider({
							auto: false,
							speed: 500,
							//infiniteLoop: true,
							autoHover: true,
							pause: 5000,
							onSlideNext: function ($slideElement, oldIndex, newIndex) {
								getJMediaList(newIndex + 1);
							},
							onSlidePrev: function ($slideElement, oldIndex, newIndex) {
								getJMediaList(newIndex + 1);
							}
						});
					}

					$('.media-list li[slide-index=' + page + ']').empty().append(d.rst);
					$(".hover-tips").fsrTip();
					showImg(page);
				},
				error: function (res) {
					console.log(res)
				}
			});
		}
    });
</script><script type="text/javascript" src="//res.jiemian.com/static/jmw/plugins/hover-tips/hover-tips.js"></script><script>
