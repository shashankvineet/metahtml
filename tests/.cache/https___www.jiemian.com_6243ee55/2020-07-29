	function myrefresh() {
	window.location.reload()
}
function ResumeError() {
	return !0
}
window.onerror = ResumeError,
		setTimeout("myrefresh()", 6e5);</script><script type="text/javascript">    var foowwLocalStorage = {
        set: function (key, value, ttl_ms) {
            if (ttl_ms) {
                var obj = {
                    data: value,
                    time: Date.now(),
                    expire: ttl_ms
                };
                //localStorage 设置的值不能为对象,转为json字符串
                localStorage.setItem(key, JSON.stringify(obj));
            }
            else {
                localStorage.setItem(key, value);
            }
        },
        get: function (key) {
            var val = localStorage.getItem(key);
            if (!val) {
                return val;
            }
            val = JSON.parse(val);
            if (Date.now() - val.time > val.expire) {
                localStorage.removeItem(key);
                return null;
            }
            return val.data;
        },
        remove: function(key) {
            localStorage.removeItem(key);
        }
    }

	if ($('#top-slide').length) {
		// var top_tg =$.cookie('top_tg');
        var top_tg = foowwLocalStorage.get('top_tg');
		if (top_tg == 1) {
			$('#top-banner').show()
			$('#top-slide').hide()
        }
        else {
			setTimeout(function() {
				$('#top-slide').slideUp(1000, function() {
					$('#top-banner').slideDown(1000);
				});
			}, 5000);
            // $.cookie('top_tg','1',{ expires: 1 })
            foowwLocalStorage.set('top_tg', '1', 60000 * 60);
		}

	}
