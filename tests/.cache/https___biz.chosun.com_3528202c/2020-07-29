<script>
function goSubmit() {
    var gsWin = window.open("about:blank", "winName");
    var ns = document.frm;
    ns.action = "//nsearch.chosun.com/search/total.search";
    ns.target = "winName";
    ns.submit();
}

$(window).load(function () {
    search_active.init();
    menu_active.init();
    gnbMenuActive();
})

function gnbMenuActive() {
    if( typeof CatID === 'undefined' || CatID === '' ){

    }else{
        var $gnbMenuList = $(".gnb_menu_list"),
            $gnbMenuListLi = $gnbMenuList.find("li");
        var getCatId= CatID.substring(0,3);
        var getCatId_F= CatID.substring(0,1);
        if(getCatId_F == 7){
            $gnbMenuListLi.eq(0).addClass("active");
        }else if(getCatId_F == 4){
            $gnbMenuListLi.eq(1).addClass("active");
        }
        else if(getCatId_F == 3){
            $gnbMenuListLi.eq(2).addClass("active");
        }
        else if(getCatId_F == 8){
            $gnbMenuListLi.eq(3).addClass("active");
        }
        else if(getCatId_F == 6){
            $gnbMenuListLi.eq(4).addClass("active");
        }
        else if(getCatId_F == 5){
            $gnbMenuListLi.eq(5).addClass("active");
        }
        else if(getCatId_F == 2){
            $gnbMenuListLi.eq(8).addClass("active");
        }
        else if(getCatId_F == 1){
            $gnbMenuListLi.eq(9).addClass("active");
        }
        else if(getCatId_F == 0){
            $gnbMenuListLi.eq(0).addClass("active");

        }else if(getCatId_F == 'C'){
            $gnbMenuListLi.eq(6).addClass("active");

        }else if(getCatId_F == 'D'){
            $gnbMenuListLi.eq(7).addClass("active");

        }else{

        }
    }
    // ì°ì : 7
    // ë¶ëì° : 4
    // ì¦ê¶ : 3
    // íí¬ : 8
    // ê¸ìµ : 6
    // ì ì± : 5
    // ìì¬ : C
    // êµ­ì  : D
    // ì¤í¼ëì¸ : 2
    // í¼í : 1
}

//========== ì¿ í¤ê° ì²ë¦¬ í¨ì E
function moveMobile(){
    try {
       var frmUrl = document.location.href;

    //    if(frmUrl.indexOf("html_dir") == -1 || typeof(ArtID) == 'undefined'){
    //       return;
    //    }

       var rsMobile = GetCookie("csMobile");
       rsMobile = rsMobile != null && rsMobile != "false" ? rsMobile : "";
       if (rsMobile == "pc") {
        //    NewsSource.init("copy", NewsSource.addSource);
           return;
       }

       var navUA = navigator.userAgent;
       var isWinCE = navUA.indexOf("Windows CE") >= 0;
       var isMobile = navUA.indexOf("Mobile") >= 0;
       var isiPAD = navUA.indexOf("iPad") >= 0;

       var _appService = "";
       if((isWinCE || isMobile) && !isiPAD) _appService = "mobile";
       var _tabList = "/SM-P605|SM-P600|SHV-E230|SHW-M480|SHW-M480W|SHW-M485W|SHW-M500W|SM-P900|SHW-M300W|SHW-M300|SHW-M305W|SHV-E140|SM-T320|SM-T310|SM-T530|SM-T335|SM-T330|SM-T800|SM-T700|SM-T255S|ASUS-1B32|ASUS-1B32-4G|ASUS-2B16|ASUS-2B32|LG-V400|LG-V480|LG-V700|LG-V500|SHW-M380|iPad|SM-T805|Nexus 9|SM-T550|SM-T555|SM-P550|SM-P555|SM-T350|SM-T355|SM-P350|SM-P355|SM-T715|SM-T815/";
       if (navUA.match(_tabList) != null) _appService = "pc";

       if ((_appService == "tablet" || _appService == "mobile") && rsMobile != "") _appService = rsMobile;

       if (_appService == "mobile") {
          var para = location.search;
          if(para != "" && para.indexOf("?") == 0) {
             para = para.substring(1);
             para = para.length > 0 ? "&"+para : "";
          }
          else para = "";
          var ref = document.referrer;
          if (ref != null && ref != "") {
             var para_ref = "", exp = /(\/\/)([^/]+)/;
             if (ref.match(exp) != null) para_ref = RegExp.$2;
             if (para_ref != "") para += "&Dep0=" + para_ref;
          }

            if(frmUrl.indexOf('utm_source') == -1 ) {
            para += '&utm_source=' + para_ref + '&utm_medium=unknown&utm_campaign=biz';
            }

          document.location.replace ("https://m.biz.chosun.com/");
          return;
       }
    }
    catch (e) {}
    // NewsSource.init("copy", NewsSource.addSource);
 }
 moveMobile();

/* ë©ë´ */
var menu_active = {
    init : function () {
        menu_active.setActive();
    },
    setActive : function () {
        var $menuBtn = $(".cbz_menu_btn a"),
            $menuList = $(".menu_list");

        $menuBtn.click(function () {
            if($menuList.hasClass("active")){
                $menuList.removeClass("active");
                $menuBtn.parent().removeClass("active");
                return false;
            }else{
                $menuList.addClass("active");
                $menuBtn.parent().addClass("active");
                return false;
            }
        });
    },
    setHeight : function () {
        var $menuList = $(".menu_list"),
            headerHeight = $("#header").innerHeight(),
            winHeight = $(window).innerHeight(),
            winWidth = $(window).innerWidth(),
            menuListHeight = winHeight - headerHeight;

        if(winWidth < 1900){
            $menuList.css("height",menuListHeight+30);
        }else{
            $menuList.css("height","auto");
        }
    },
    setScroll : function () {
        var $scrollWrap = $(".menu_list");
        $scrollWrap.mCustomScrollbar({
            theme:"minimal-dark",
            scrollButtons:{
                enable:true
            },
            mouseWheelPixels: 300
        })
    }
}

/* ê²ì */
var search_active = {
    init : function () {
        search_active.setActive();
    },
    setActive : function () {
        var $searchBtn = $(".cbz_search_btn"),
            $searchArea = $(".sch_area"),
            $wrapper = $("#wrapper");

        $searchBtn.click(function () {
            if($searchArea.hasClass("active")){
                $searchBtn.removeClass("active");
                $searchArea.removeClass("active");
                $("#bg_mask").removeClass("active");
                $("#bg_mask").remove();
                return false;
            }else{
                $searchBtn.addClass("active");
                $searchArea.addClass("active");
                $("#bg_mask").addClass("active");
                $wrapper.append('<div id="bg_mask" class="active"></div>');
                search_active.setMaskHeight();

                $("#bg_mask").click(function () {
                    $searchBtn.removeClass("active");
                    $searchArea.removeClass("active");
                    $(this).removeClass("active");
                    $(this).remove();
                });
                return false;
            }
        })
    },
    setMaskHeight : function () {
        $("#bg_mask").height($(document).height()-$("#header").innerHeight());
    }
}

$(window).scroll(function(){
    var headerFixed = $('.gnb_area'),
        scroll = $(window).scrollTop();

    if (scroll >= 130){
        headerFixed.addClass('fixed');
        // headerFixed.css("opacity","0");
        $(".sch_area").addClass("fixed");
    }else{
        headerFixed.removeClass('fixed');
        // headerFixed.css("opacity","1");
        $(".sch_area").removeClass("fixed");
    }

    if($(".result_head").length>0 && scroll > 130){
        headerFixed.css("opacity","0");
    }
});
</script>

