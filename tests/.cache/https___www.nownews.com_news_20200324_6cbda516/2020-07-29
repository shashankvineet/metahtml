<script type="text/javascript">
    function adReport(adKey,action){
        var nn_id = $.cookie('nn_id');
        if(!nn_id){
            $.cookie('nn_id', uuidv4(), { expires: 3650, path: '/' });
        }
        nn_id = $.cookie('nn_id');
        $.ajax({
            type: "POST",
            url: cloudfuncAdUrl,
            data: JSON.stringify({
                topic: "adInfo",
                message: {
                    viewTime:dayjs().format('YYYY/MM/DD HH:mm:ss'),
                    pagePath:location.href,
                    device:"pc",
                    cookieId:"nn_id="+nn_id,
                    openId:"",
                    deviceId:"",
                    remoteIP:returnCitySN["cip"],
                    adKey:adKey,
                    action:action?action:'load'
                }
            }),
            contentType: "application/json",
            success: function(msg){
            },
            error:function (msg) {
            }
        });
    }
</script>
