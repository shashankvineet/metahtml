<script id="cc-m-reg-10119714597">// <![CDATA[

    window.regModule("module_gallery", {"view":"3","countImages":4,"variant":"cool","selector":"#cc-m-gallery-10119714597","imageCount":4,"enlargeable":"1","multiUpload":true,"autostart":1,"pause":"4","showSliderThumbnails":1,"coolSize":"2","coolPadding":"3","stackCount":"3","stackPadding":"3","options":{"pinterest":"0"},"id":10119714597});
