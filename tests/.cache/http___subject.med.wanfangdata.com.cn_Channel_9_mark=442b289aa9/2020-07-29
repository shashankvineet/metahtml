    $('#search-highlight').hideseek({ //检索
        highlight: true
    });
    //获取URL中的参数方法
    function getQueryString(name) {
        let reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
        let r = window.location.search.substr(1).match(reg);
        if (r != null) {
            return unescape(r[2]);
        }
        return -1;
    }
 			//获取URL中的mark参数
        var mark = getQueryString("mark");//44
           //判断设备
        var userAgentInfo = navigator.userAgent;
        var Agents = new Array("Android", "iPhone", "SymbianOS", "Windows Phone", "iPad", "iPod");
        var flag = false;
        var v = 0;
        for (v = 0; v < Agents.length; v++) {
            if (userAgentInfo.indexOf(Agents[v]) > 0) { flag = true; break; }
        }
        //版块页的逻辑代码实现
       if (mark != -1) {
         $(".expert").hide();
            $(".sort").hide();
            $(".newest").hide();
            $(".section").show();
            $(".banner").hide();
            $(".contents").hide();
            $("body").show();
       	$('.origin').perfectScrollbar();//滚动轴
          var tagArry = [];
          var origin = [];
          $(".section .item_detail_list li").each(function () {
              if ($(this).attr("mark") == mark) {
               	  $(this).show();
                  var originStr = $(this).children("div").children(".subtitle").children(".origin").text().replace('来源：', '');
                  if (origin.indexOf(originStr) == -1) {
                      origin.push(originStr);
                  }
                   $(this).attr("origin", originStr);
                  var $Tag = $(this).children(".tag");
                    if($Tag.attr("text") != undefined){
                    	  var text = $Tag.attr("text").split(','),
                      str = "";
                  $(this).attr("stamp", $Tag.attr("text"));
                  for (var i = 0; i < text.length; i++) {
                      str += '<button>' + text[i] + '</button>';
                      if (tagArry.indexOf(text[i]) == -1 && text[i] != "") {
                          tagArry.push(text[i]);
                      }
                  }
                  $Tag.html(str);
                    }
                
              }
          });
          var str = "";
          for (var i = 0; i < tagArry.length; i++) {
              if(tagArry[i]=="外文" || tagArry[i]=="中文"||tagArry[i]=="中文文献" || tagArry[i]=="外文文献" || tagArry[i]=="中文指南" || tagArry[i]=="外文指南"){
                  str += '<li mark="' + tagArry[i] + '">' + tagArry[i] + '<svg t=\"1582115504996\" class=\"icon\" viewBox=\"0 0 1024 1024\" version=\"1.1\" xmlns=\"http://www.w3.org/2000/svg\" p-id=\"2826\" width=\"16\" height=\"16\"><path d=\"M377.47806845 789.11115837l279.65492248-279.65492248-278.8075161-278.8075161z\" p-id=\"2827\" fill=\"#b1d2f2\"></path></svg></li>';
              }
          }
          $(".section .assort ul.stamp").html(str);
          $(".section .assort ul.stamp li").click(function () {
                  $("html, body").animate({
                      scrollTop: 0
                  }, {duration: 500, easing: "swing"});
              if($(this).hasClass("active")){
                  $(this).removeClass('active');
                  $(this).parent().siblings("span").text('类型');
                  $(".section .item_detail_list li").each(function () {
                      $(this).show();
                  })
              }else{
                  $(this).addClass('active').siblings().removeClass('active');
                  var stamp = $(this).attr("mark");
                  $(this).parent().siblings("span").text(stamp);
                  var originMark = $(".section .assort ul.origin li.active").attr("mark");
                  $(".section .item_detail_list li").each(function () {
                      var stampEc = $(this).attr("stamp");
                      var originEc = originMark != undefined ? $(this).attr("origin") : undefined;
                      if (stampEc != undefined) {
                          if (stampEc.indexOf(stamp) == -1) {
                              $(this).hide();
                          } else {
                              if (originMark == originEc || originEc == undefined) {
                                  $(this).show();
                              }
                          }
                      }
                  });
              }
          });
          var str = "";
          for (var i = 0; i < origin.length; i++) {
              str += '<li mark="' + origin[i] + '">' + origin[i] + '<svg t=\"1582115504996\" class=\"icon\" viewBox=\"0 0 1024 1024\" version=\"1.1\" xmlns=\"http://www.w3.org/2000/svg\" p-id=\"2826\" width=\"16\" height=\"16\"><path d=\"M377.47806845 789.11115837l279.65492248-279.65492248-278.8075161-278.8075161z\" p-id=\"2827\" fill=\"#b1d2f2\"></path></svg></li>';
          }
          $(".section .assort ul.origin").html(str);
          $(".section .assort ul.origin li").click(function () {
              $("html, body").animate({
                      scrollTop: 0
                  }, {duration: 500, easing: "swing"});
              if($(this).hasClass("active")){
