        var domList = $('.pc .newest .item_list li').get();
        domList.sort(function(a,b){
            var elOne = new Date($(a).attr("time")).getTime();
            var elTwo = new Date($(b).attr("time")).getTime();
            if(elOne > elTwo) return -1;
            if(elOne < elTwo) return 1;
            return 0;
        });
