            $(this).children(".option ").show();
        }else{
            $(this).children(".option ").hide();
        }
    })
    function getScrollTop() {
        var scrollPos;
        if (window.pageYOffset) {
            scrollPos = window.pageYOffset;
        } else if (document.compatMode && document.compatMode != 'BackCompat') {
            scrollPos = document.documentElement.scrollTop;
        } else if (document.body) {
            scrollPos = document.body.scrollTop;
        }
        return scrollPos;
    }
    window.onscroll = function () {//监听滚动条
        console.log(getScrollTop());
        if (getScrollTop() > 20) {//当滚动条离开顶端时，显示"回到顶部"。这里写20的原因是，"回到顶端"按钮也不是需要马上出现，根据自己的需求 而设置。
            $("#top").show();
        } else {//当滚动条回到顶端时，将"回到顶端按钮"  隐藏
            $("#top").hide();
        }
        if (getScrollTop() > 170) {
                $(".float-wrap").addClass("left-bar-list");
        }else {
            $(".float-wrap").removeClass("left-bar-list");
        }
    };
    $("#top").click(function () {
        $("html, body").animate({
            scrollTop: 0
        }, {duration: 500, easing: "swing"});
    })
