                  var stampMark = $(".section .assort ul.stamp li.active").attr("mark");
                  $(this).parent().siblings("span").text(origin);
                  $(".section .item_detail_list li").each(function () {
                      var originEC = $(this).attr("origin");
                      var stampEc = stampMark != undefined ? $(this).attr("stamp") : undefined;
                      if (originEC != undefined) {
                          if (originEC != origin) {
                              $(this).hide();
                          } else {
