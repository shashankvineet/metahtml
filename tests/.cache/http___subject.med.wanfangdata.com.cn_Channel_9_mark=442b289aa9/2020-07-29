                $(".sort p").each(function () {
                    if ($(this).attr('mark') == mark) {
                        $("html, body").animate({
                            scrollTop: $(this).offset().top
                        }, {duration: 500, easing: "swing"});
                    }
                });
            } else {
                $(".expert p").each(function () {
                    if ($(this).attr('mark') == mark) {
                        $("html, body").animate({
                            scrollTop: $(this).offset().top
                        }, {duration: 500, easing: "swing"});
                    }
                });
            }
        }else{
            if (mark != "42" && mark != "36") {
                window.location.href = "?mark=" + mark;
            }else if(mark == "36"){
                window.location.href = "/Channel/8/";
            }else if(mark == "42"){
                window.location.href = "/Plate/42/";
            }
        }
    });
    $(".assort .btn").click(function () {
