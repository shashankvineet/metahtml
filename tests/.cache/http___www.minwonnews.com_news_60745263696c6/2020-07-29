         <script type="text/javascript"> 
         /////////////////////////////////////////////////////  id- 인도네시아, km 캄보디아  , lo 라오스, ms - 말레이시아,   th 태국, tl 필리핀,   vi - .베트남
        function googleTranslateElementInit() {
            $.when(
                new google.translate.TranslateElement({pageLanguage: 'ko', includedLanguages: 'ko,en,ja,zh-CN',
                    layout: google.translate.TranslateElement.InlineLayout.SIMPLE, autoDisplay: false, multilanguagePage: true}, 'google_translate_element')
            ).done(function(){
                //$('.translation-icons a.us').click(); 
            });
        }
		 ////////////////////////////////////////////////////


          </script>
           <script type="text/javascript"> 
              $(window).load(function () {
			  
              $('.translation-icons').css('visibility', 'visible'); 
              $('.translation-icons a').click(function(e) { 
                 e.preventDefault(); 
                 var placement = $(this).data('placement'); 
                 var lang_num = $('.translation-icons a').length; 
                 var $frame = $('.goog-te-menu-frame:first'); 

                 if (!$frame.size()) { 
                 alert("Error: Could not find Google translate frame."); 
                 return false; 
                 } 
                 
				 // id- 인도네시아, km 캄보디아  , lo 라오스, ms - 말레이시아,   th 태국, tl 필리핀,   vi - .베트남
                 var langs = $('.goog-te-menu-frame:first').contents().find('a span.text');
                 if(langs.length != lang_num) placement = placement+1; 
                 langs.eq(placement).click(); 
				 //세션생성
				 //alert(placement);
								 if(placement==0) {
				 var site_lang = 1;
				 }else if(placement==1) {
				 var site_lang = 2;
				 }else if(placement==2) {
				 var site_lang = 3;
				 }else if(placement==3) {
				 var site_lang = 4;
				 }
				 				 //}

                 sessionLang('site_language',site_lang,'','1');
                 return false; 
              }); 

           });
           </script> 
           
