<script type="text/javascript">
/**
* wml_Tag,首页上边点击每个领域下边的重要资源、重要活动、热点词都只用这一个js函数
* @param showmcontent
* @param indexId
* @param selfObj
*/
function wml_TagSh(showmcontent,indexId,dirId,indexNum,selfObj) {///////////tags1
// 操作标签
//alert("indexId"+indexId+"************");
var elementId="index_tags"+indexId;
// alert("elementId"+elementId+"=========");
var tag = document.getElementById(elementId).getElementsByTagName("li");
for (i = 0; i < tag.length; i++) {
tag[i].className = "";
}
//selfObj.parentNode.className = "w_tag";
tag[selfObj].className="w_tag";
// 操作内容
var tagId="tag"+indexId+"_li";
// alert("tagId:"+tagId);
for (i = 0; j = document.getElementById(tagId + i); i++) {	
j.style.display = "none";
}

document.getElementById(showmcontent).style.display = "block";

getInfo(showmcontent,dirId,indexNum);
}

var hasAppend={};
function getInfo(showmcontent,dirId,indexNum){
	var url='/STMonitor/openhome/ajax/getHomeDirInfo.htm';
	var parpageId='1596049194225';
	var div = $('#'+showmcontent);
	if(indexNum!=1&&indexNum!=2&&indexNum!=3&&indexNum!=0)return;
	//if(hasAppend[showmcontent])return;
	var type='hotTopic';
	if(indexNum==1)type='object';
	//if(indexNum==3)type='newItem';
	if(indexNum==3)type='lastedRecomendInfo';
	if(indexNum==0)type='newItem';

		
	$.ajax({    
           type:"POST",    
           url:url,    
           dataType:"json",
           data:{type:type,directionId:dirId},
           beforeSend:function(){  
           		div.empty();         	
           		div.append("加载数据......");
           },
           error:function(){
           		div.empty();
           		div.append($('<a href="javascript:void(0)" onclick="getInfo(\''+showmcontent+'\',\''+dirId+'\',\''+indexNum+'\')" >加载出错！<br/>点我重新加载</a>'));
           },    
           success:function(rInfo){
	           	hasAppend[showmcontent+'']=true;	
	           	div.empty();
	           	if(rInfo.length==0){
           			div.append($("<p>暂无数据</p>"));
           			return;
           		}
	         	
	         	
	           	var dd =$("<dd></dd>");
	           	if(indexNum==1||indexNum==2){//重要活动和热点词
	           		for(var i=0;i<rInfo.length;i++){
		           		var label=$("<label></label>");
		           		var aLink=$('<a   href="/STMonitor/openhome/directionAttention.htm'
		           							+'?parentPageId='+parpageId
		           							+'&directionId='+dirId
		           							+'&type='+type
		           							+'&objectId='+rInfo[i].id+'"'
		           							+'  target="_blank" title="'+rInfo[i].title+'" >'
											+ rInfo[i].title
											+'</a>');
						var em=$('<em>('+rInfo[i].num+');</em>');
						if(indexNum==1){
							em.prepend('['+rInfo[i].type+']');
						}
						aLink.append(em);						
		           		label.append(aLink);
		           		div.append(label);
		           		
		           		
		           	}
	           		var moreLink =$('<a name="更多" class="w_more5" href="/STMonitor/openhome/directionAttention.htm?'
		            				+'parentPageId='+ parpageId
		            				+'&directionId='+dirId
									+'&type='+type+'"'
									+'>查看更多领域关注&nbsp;</a>');
	           		div.append(moreLink);
	           	}

	           	if(indexNum==0){//最新消息		         
	           		var ddlabel=$("<dd></dd>");
					for(var i=1;i<rInfo.length;i++){
		           		var label=$("<label></label>");
		           		var aLink=$('<em>'+rInfo[i].publishTime+'</em>&nbsp;&nbsp;<a  href="/STMonitor/openhome/originalredirect.htm?'
		           							+'id='+rInfo[i].id+'"'
		           							+'  target="_blank"  title="'+rInfo[i].title+'">'
											+ rInfo[i].title
											+'</a>');
		           		label.append(aLink);
		           		ddlabel.append(label);
		           		div.append(ddlabel);
		           	}
					
					
	           		var moreLink =$('<a name="更多" class="w_more2" href="/STMonitor/openhome/directionInfo.htm?'
		            				+'parentPageId='+ parpageId
		            				+'&directionId='+dirId+'"'
									+'>查看更多领域信息&nbsp;</a>');
	           		div.append(moreLink);
				}

	          
	        	if(indexNum!=3)
	        		div.append(dd);

				/*	
				if(indexNum==3){//最新推荐——编译报道
						for(var i=0;i<1;i++){
							var dt =$("<dd id='picdd"+dirId+"' style='height:165px;'></dd>");
							var imageUrl;
							
							if(typeof(rInfo[i].imageURL)!="undefined" && rInfo[i].imageURL!=0)
								imageUrl="../doc/user/"+rInfo[i].imageURL;
							else
								{
								imageUrl="../doc/user/"+rInfo[i].defaultImageUrl;
								}
							
			           		var bLink=$('<div style="float:left; display:inline; margin-top:0; margin-left:5px;width:130px;"><img  src="'+imageUrl+'"  title="'+rInfo[i].imageTitle+'" class="index_img" height="181" width="145"/></div>');
			           		dt.append(bLink);
			           		var blabel=$('<div style="float:left; display:inline; margin-top:20; margin-left:30px;width:160px;">'+
				         			   '<a href="/STMonitor/home/bianyi_recordshow.htm?'
			           							+'id='+rInfo[i].id+'&parentPageId='+ parpageId+'&controlType=openhome"'
			           							+'  target="_blank" title="'+rInfo[i].title+'">&nbsp;&nbsp;<b>'
												+ rInfo[i].title+'('+rInfo[i].publishTime+')'
												+'</b></a>'+
				         			                '<div style="text-indent:15px;" >'+rInfo[i].content
												+'</div>'+
				         			            '</div>');
			           		dt.append(blabel);
			           		div.append(dt);
			           	}
					}
					
				if(indexNum==3){
					var ddlabel=$("<dd></dd>");
						for(var i=1;i<rInfo.length;i++){
							var label=$("<label onmouseover=\"setPicDDContent(\'"+rInfo[i].id+"\',\'"+dirId+"\')\"></label>");
			           		var aLink=$('<em>'+rInfo[i].publishTime+'</em>&nbsp;&nbsp;<a  href="/STMonitor/home/bianyi_recordshow.htm?'
			           							+'id='+rInfo[i].id+'&parentPageId='+ parpageId+'&controlType=openhome"'
			           							+'  target="_blank"  title="'+rInfo[i].title+'">'
												+ rInfo[i].title
												+'</a>');
			           		label.append(aLink);
			           		ddlabel.append(label);
			           	}
						div.append(ddlabel);
						
		           		var moreLink =$('<a name="更多" class="w_more2" href="/STMonitor/home/resource_more_recommendadd.htm?'
			            				+'parentPageId='+ parpageId
			            				+'&directionId='+dirId+'&controlType=openhome"'
										+'>查看更多领域推荐&nbsp;</a>');
						div.append(moreLink);
					}	         
	        	*/
      		}
	      		
      	});
      	
} 
