$(document).ready(function(){
	getUserCenterList();
	
	//加载最新信息
	
		 //getInfo('tag0_li0','2',0);
		 wml_TagSh('tag0_li0','0','2',0,0);
	
		 //getInfo('tag1_li0','3',0);
		 wml_TagSh('tag1_li0','1','3',0,0);
	
		 //getInfo('tag2_li0','8',0);
		 wml_TagSh('tag2_li0','2','8',0,0);
	
		 //getInfo('tag3_li0','11',0);
		 wml_TagSh('tag3_li0','3','11',0,0);
	
		 //getInfo('tag4_li0','12',0);
		 wml_TagSh('tag4_li0','4','12',0,0);
	
		 //getInfo('tag5_li0','13',0);
		 wml_TagSh('tag5_li0','5','13',0,0);
	

	//var t=setTimeout("checkHasContent()",2000);
	
});


function checkHasContent(){
		
		if($('#tag0_li3').text()=='暂无数据'){
			
			wml_TagSh('tag0_li0','0','2',0,1);
		}	
		 
		
		if($('#tag1_li3').text()=='暂无数据'){
			
			wml_TagSh('tag1_li0','1','3',0,1);
		}	
		 
		
		if($('#tag2_li3').text()=='暂无数据'){
			
			wml_TagSh('tag2_li0','2','8',0,1);
		}	
		 
		
		if($('#tag3_li3').text()=='暂无数据'){
			
			wml_TagSh('tag3_li0','3','11',0,1);
		}	
		 
		
		if($('#tag4_li3').text()=='暂无数据'){
			
			wml_TagSh('tag4_li0','4','12',0,1);
		}	
		 
		
		if($('#tag5_li3').text()=='暂无数据'){
			
			wml_TagSh('tag5_li0','5','13',0,1);
		}	
		 
	
	
}
function getUserCenterList(){
	var div=$("#search_key_1");
	var url='/STMonitor/openhome/ajax/getHomeDirInfo.htm';
	var type='userCenter';
	var dirId='75;99;108;109;111;113;143;144;146;147;148;149;151;152;153;154;163;168;169;170;172;175;176';
	
	$.ajax({    
           type:"POST",    
           url:url,    
           dataType:"json",
           data:{type:type,directionId:dirId},
           beforeSend:function(){  
           		div.empty();         	
           		div.append("加载数据......");
           	
           },
           error:function(XMLHttpRequest, textStatus, errorThrown){
           		div.empty();
           		div.append($('<a href="javascript:void(0)" onclick="getUserCenterList()" >加载出错！<br/>点我重新加载</a>'));
           },    
           success:function(rInfo){
	           	div.empty();
	           	if(rInfo.length==0){
           			div.append($("<p>暂无数据</p>"));
           			return;
           		}
	           	
	           	
           		for(var i=0;i<rInfo.length;i++){
           			var label=$("<dd></dd>");
           			var aLink=$('<a  href="/STMonitor/index.htm'
				    		+'?serverId='+rInfo[i].serverId+'"'
				    		+'>'
				    		+rInfo[i].serverName
				    		+'</a>');
	           		
           			label.append(aLink);
	           		div.append(label);
           			//div.append(aLink);
	           	}
				
           	
      		}
      	});
	 
	              	
}


function setPicDDContent(id,dirId){
	
	var url='/STMonitor/openhome/ajax/getHomeDirInfo.htm';
	var parpageId='1596049194225';
	var showmcontent='picdd'+dirId;
	var div = $('#picdd'+dirId);

	//置空picdd	
	div.empty();
	var type='recomendInfoDetail';
	
			
	$.ajax({    
           type:"POST",    
           url:url,    
           dataType:"json",
           data:{type:type,directionId:dirId,recid:id},
           beforeSend:function(){  
           		div.empty();         	
           		div.append("加载数据......");
           },
           error:function(){
           		div.empty();
           		div.append($('<a href="javascript:void(0)" onclick="getInfo(\''+showmcontent+'\',\''+dirId+'\',\''+indexNum+'\')" >加载出错！<br/>点我重新加载</a>'));
           },    
           success:function(rInfo){
	           	hasAppend[showmcontent+'']=true;	
	           	div.empty();
	           	if(rInfo.length==0){
           			div.append($("<p>暂无数据</p>"));
           			return;
           		}

	         	for(var i=0;i<1;i++){
							
							var imageUrl;
							
							if(typeof(rInfo[i].imageURL)!="undefined" && rInfo[i].imageURL!=0)
								imageUrl="../doc/user/"+rInfo[i].imageURL;
							else
								{
								imageUrl="../doc/user/"+rInfo[i].defaultImageUrl;
								}
							
			           		var bLink=$('<div style="float:left; display:inline; margin-top:0; margin-left:5px;width:130px;"><img  src="'+imageUrl+'"  title="'+rInfo[i].imageTitle+'" class="index_img" height="161" width="145"/></div>');
			           		div.append(bLink);
			           		var blabel=$('<div style="float:left; display:inline; margin-top:20; margin-left:30px;width:160px;">'+
				         			   '<a href="/STMonitor/home/bianyi_recordshow.htm?'
			           							+'id='+rInfo[i].id+'&parentPageId='+ parpageId+'&controlType=openhome"'
			           							+'  target="_blank" title="'+rInfo[i].title+'">&nbsp;&nbsp;<b>'
												+ rInfo[i].title+'('+rInfo[i].publishTime+')'
												+'</b></a>'+
				         			                '<div style="text-indent:15px;" >'+rInfo[i].content
												+'</div>'+
				         			            '</div>');
			           		div.append(blabel);
			           	
			 }
          
           }	
       	});	
   	
	
}
</script>

</html>