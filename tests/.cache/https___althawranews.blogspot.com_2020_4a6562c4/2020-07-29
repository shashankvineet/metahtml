$(window).load(function() {
    $('.owl-carousel').owlCarousel({
    animateOut: 'fadeIn',
    animateIn: 'fadeOut',
    items:1,
 autoplay:true,
    autoplayTimeout:5000,
    stagePadding:0,
      nav:true,
      loop:true,
      navText:["", ""]
