$(".item-box").hover(
  function () {

	$(this).children().find('.box_thumb').addClass('hide'); 
    $(this).children().find('.box_desc').removeClass('hide'); 
	
  },
  function () {

	$(this).children().find('.box_thumb').removeClass('hide'); 
    $(this).children().find('.box_desc').addClass('hide'); 

  }
);

