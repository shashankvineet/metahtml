		slideWidth: 164,
		minSlides: 7,
		maxSlides: 7,
		moveSlides: 1,
		slideMargin: 17.3,
		onSlideAfter: function(){
			 setTimeout(function(){
				$('.slide01 .bx-start').trigger('click');
			 },1000);
		  }
	});
	
