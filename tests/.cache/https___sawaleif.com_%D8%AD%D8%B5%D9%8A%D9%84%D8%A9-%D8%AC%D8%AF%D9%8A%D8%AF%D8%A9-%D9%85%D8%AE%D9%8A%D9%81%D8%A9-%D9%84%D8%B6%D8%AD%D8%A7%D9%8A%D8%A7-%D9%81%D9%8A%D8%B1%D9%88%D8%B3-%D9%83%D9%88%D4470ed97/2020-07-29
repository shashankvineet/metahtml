						frame.src = "https://jetpack.wordpress.com/jetpack-comment/?blogid=99653547&postid=530288&comment_registration=0&require_name_email=1&stc_enabled=0&stb_enabled=0&show_avatars=1&avatar_default=mystery&greeting=%D8%A7%D8%AA%D8%B1%D9%83+%D8%B1%D8%AF%D8%A7%D9%8B&greeting_reply=%D8%A7%D8%AA%D8%B1%D9%83+%D8%B1%D8%AF%D8%A7%D9%8B+%D8%B9%D9%84%D9%89+%25s&color_scheme=light&lang=ar&jetpack_version=8.7.1&show_cookie_consent=10&has_cookie_consent=0&token_key=%3Bnormal%3B&sig=0bed3c5bfc0e4dfc6ba795f702a40b4a0e7753d4#parent=https%3A%2F%2Fsawaleif.com%2F%25D8%25AD%25D8%25B5%25D9%258A%25D9%2584%25D8%25A9-%25D8%25AC%25D8%25AF%25D9%258A%25D8%25AF%25D8%25A9-%25D9%2585%25D8%25AE%25D9%258A%25D9%2581%25D8%25A9-%25D9%2584%25D8%25B6%25D8%25AD%25D8%25A7%25D9%258A%25D8%25A7-%25D9%2581%25D9%258A%25D8%25B1%25D9%2588%25D8%25B3-%25D9%2583%25D9%2588%25D8%25B1%25D9%2588%25D9%2586-530288%2F" + '&replytocom=' + parseInt( comm_par, 10 ).toString();
