			// 			$('.ad-banner-widget-5 .w-ad-banner__wrap').css({left:$ad5Width*liIndex*-1});
			// 		}
			// 	}

			// 		window.sessionStorage.setItem('ad5Pos',liIndex);

			
			if(liIndex <= 0 ){
				liIndex=$ad5Count - 1;
				ad5wrap.addClass('noTransition');
				setTimeout(gotoLast,1);
				function gotoLast(){
					ad5wrap.css({left:$ad5Width*liIndex*-1});
					setTimeout(function(){						
						ad5wrap.removeClass('noTransition');
							liIndex=($ad5Count-1) - 1;
							ad5wrap.css({left:$ad5Width*liIndex*-1});
							window.sessionStorage.setItem('ad5Pos',liIndex);
						},50);

				}
			}else{
				liIndex--;
				ad5wrap.css({left:$ad5Width*liIndex*-1});
				window.sessionStorage.setItem('ad5Pos',liIndex);
			}



		}
		function nextClick(){
			liIndex++;
			ad5wrap.removeClass('noTransition');
			ad5wrap.css({left:$ad5Width*liIndex*-1});

				if(liIndex >= $ad5Count -1 ){
					setTimeout(function(){
						liIndex=0;
						window.sessionStorage.setItem('ad5Pos',liIndex);	
						ad5wrap.addClass('noTransition');
					},300);
					setTimeout(gotoOne,301);
					function gotoOne(){
						ad5wrap.css({left:0});
					}
				}
				else{

					window.sessionStorage.setItem('ad5Pos',liIndex);	
				}


		
             
						
		}
		$('#ad5-prev').on({'click':prevClick});
		$('#ad5-next').on({'click':nextClick});
		// var ads= $('.ads');
		// for(var i=0,max=ads.length;i<max;i++){

		// 	var adsa = ads.eq(i).find('.slide');
		// 	adsa.hover(function(){
	 //            var index = $(this).index();

	 //            adsa.eq(index).siblings().toggleClass('active');
	 //        })			
		// }
		$('.slide').hover(function(){
			$(this).siblings().toggleClass('active');
		})


})
$(window).resize(function(){
		var $ad5Width = $('.ad-banner-widget-5').outerWidth();
		var $ad5Count = $('.ad-banner-widget-5 .ads').length;
		console.log($ad5Count);
		var $ad5Height = $('.ad-banner-widget-5 .w-ad-banner__wrap').outerHeight();
		var liIndex = window.sessionStorage.getItem('ad5Pos');
		var ad5wrap = $(".ad-banner-widget-5 .w-ad-banner__wrap");
        $('.ad-banner-widget-5').css({height:$ad5Height});
		ad5wrap.width( $ad5Width * $ad5Count );
		$('.ad-banner-widget-5 .ads').width( $ad5Width );
		ad5wrap.css({left:$ad5Width*liIndex*-1});
		$('#ad5-prev').on({'click':prevClick});
		$('#ad5-next').on({'click':nextClick});	
		function prevClick(){

			if(liIndex <= 0 ){
				liIndex=$ad5Count - 1;
				ad5wrap.addClass('noTransition',gotoLast);
				function gotoLast(){
					ad5wrap.css({left:$ad5Width*liIndex*-1});
					setTimeout(function(){						
						ad5wrap.removeClass('noTransition');
							liIndex=($ad5Count-1) - 1;
							ad5wrap.css({left:$ad5Width*liIndex*-1});
							console.log(liIndex);
							window.sessionStorage.setItem('ad5Pos',liIndex);
						},50);

				}
			}else{
				liIndex--;
				ad5wrap.css({left:$ad5Width*liIndex*-1});
				window.sessionStorage.setItem('ad5Pos',liIndex);
			}

		
             
		}
		function nextClick(){

			liIndex++;
			ad5wrap.removeClass('noTransition');
			ad5wrap.css({left:$ad5Width*liIndex*-1});

				if(liIndex >= $ad5Count -1 ){
					setTimeout(function(){
						liIndex=0;
						window.sessionStorage.setItem('ad5Pos',liIndex);	
					ad5wrap.addClass('noTransition',gotoOne)
						function gotoOne(){
							ad5wrap.css({left:0});
						}
					},300);
				}
				else{

					window.sessionStorage.setItem('ad5Pos',liIndex);	
				}


		
		}		
})
</script>

</aside>
        </div>
    </div>

  </div>
  
