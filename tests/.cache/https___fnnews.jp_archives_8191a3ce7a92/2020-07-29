.pc #global_menu ul li.menu-category-1 a { background:#7B93A0; } .pc #global_menu ul li.menu-category-1 a:hover { background:#8DA9B8; } .flex-control-nav p span.category-link-1 { color:#7B93A0; } #index-category-post-1 .headline1 { border-left:5px solid #7B93A0; } #index-category-post-1 a:hover { color:#7B93A0; } .category-1 a:hover { color:#7B93A0; } .category-1 .archive_headline { background:#7B93A0; } .category-1 #post_list a:hover { color:#7B93A0; } .category-1 .post a { color:#7B93A0; } .category-1 .post a:hover { color:#8DA9B8; } .category-1 .page_navi a:hover { color:#fff; background:#7B93A0; } .category-1 #guest_info input:focus { border:1px solid #7B93A0; } .category-1 #comment_textarea textarea:focus { border:1px solid #7B93A0; } .category-1 #submit_comment:hover { background:#7B93A0; } .category-1 #previous_next_post a:hover { background-color:#7B93A0; } .category-1 #single_author_link:hover { background-color:#7B93A0; } .category-1 #single_author_post li li a:hover { color:#7B93A0; } .category-1 #post_pagination a:hover { background-color:#7B93A0; } .category-1 #single_title h2 { background:#7B93A0; } .category-1 .author_social_link li.author_link a { background-color:#7B93A0; } .category-1 .author_social_link li.author_link a:hover { background-color:#8DA9B8; } .pc #global_menu ul li.menu-category-1363 a { background:#BB6969; } .pc #global_menu ul li.menu-category-1363 a:hover { background:#C27E7E; } .flex-control-nav p span.category-link-1363 { color:#BB6969; } #index-category-post-1363 .headline1 { border-left:5px solid #BB6969; } #index-category-post-1363 a:hover { color:#BB6969; } .category-1363 a:hover { color:#BB6969; } .category-1363 .archive_headline { background:#BB6969; } .category-1363 #post_list a:hover { color:#BB6969; } .category-1363 .post a { color:#BB6969; } .category-1363 .post a:hover { color:#C27E7E; } .category-1363 .page_navi a:hover { color:#fff; background:#BB6969; } .category-1363 #guest_info input:focus { border:1px solid #BB69