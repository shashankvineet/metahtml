                    function openNav1() {
                                    var width = window.innerWidth;
                    var height = window.innerHeight;
                                    if (width <= 700) {
                        document.getElementById("Div1").style.width = "100%";
                    }
                                    else {
                        document.getElementById("Div1").style.width = "100%";
                    }

                }

                                function closeNav1() {
                        document.getElementById("Div1").style.width = "0%";
                    }
    </script>
