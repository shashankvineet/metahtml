        $(document).ready(function () {

            $(document).on('click', '[data-action-navigate-to]', function (e) {
                var el = $(this);

                var navigateToString = el.data('action-navigate-to');

                if (navigateToString !== "") {
                    window.location = navigateToString;

                } else {
                    e.stopImmediatePropagation();
                }
            })
        });

