  jQuery_1_7_2(document).ready(function(){
    jQuery_1_7_2("#leftsead a").hover(function(){
      if(jQuery_1_7_2(this).prop("className")=="weibo"){
        jQuery_1_7_2(this).children("img.hides").show();
      }else if(jQuery_1_7_2(this).prop("className")=="weixin"){
        jQuery_1_7_2(this).children("img.hides").show();
      }else if(jQuery_1_7_2(this).prop("className")=="dl"){
        jQuery_1_7_2(this).children("img.hides").show();
      }else if(jQuery_1_7_2(this).prop("className")=="app"){
        jQuery_1_7_2(this).children("img.hides").show();
      }else{
        jQuery_1_7_2(this).children("img.hides").show();
        jQuery_1_7_2(this).children("img.shows").hide();
        jQuery_1_7_2(this).children("img.hides").animate({marginRight:'0px'},'slow');
      }
    },function(){
      if(jQuery_1_7_2(this).prop("className")=="weibo"){
        jQuery_1_7_2(this).children("img.hides").hide('slow');
      }else if(jQuery_1_7_2(this).prop("className")=="weixin"){
        jQuery_1_7_2(this).children("img.hides").hide('slow');
      }else if(jQuery_1_7_2(this).prop("className")=="dl"){
        jQuery_1_7_2(this).children("img.hides").hide('slow');
      }else if(jQuery_1_7_2(this).prop("className")=="app"){
        jQuery_1_7_2(this).children("img.hides").hide('slow');
      }else{
        jQuery_1_7_2(this).children("img.hides").animate({marginRight:'-143px'},'slow',function(){jQuery_1_7_2(this).hide();jQuery_1_7_2(this).next("img.shows").show();});
      }
    });
    
    jQuery_1_7_2("#top_btn").click(function(){if(scroll=="off") return;jQuery_1_7_2("html,body").animate({scrollTop: 0}, 600);});
    
  });
  
