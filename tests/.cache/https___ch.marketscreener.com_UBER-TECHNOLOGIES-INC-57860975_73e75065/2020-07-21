    Change3charts = function()
    {   
      var mdiv;
      this.OnSuccess = function()
      {
         var texte = this.GetResponseText();
         document.getElementById("3charts_"+mdiv).innerHTML = texte;  
      }

      this.Exec = function(valueAdd,tdiv)
      {
         mdiv=tdiv;
         var uri = '/mods_a/charts/3charts_a.php?ModsConfig=true&'+valueAdd+'&mdiv='+mdiv;
         this.InitializeRequest('POST', uri);
         this.Commit(null);        
         return false;   
      }
    } 
    Change3charts.prototype = new ajax();   
    myChange3charts = new Change3charts();   
    </script>
