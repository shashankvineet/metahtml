                 data-readingaid="desktop"
                 data-readingaid-height="220"
                 data-accordion="{'hideOthers': false, 'onlyViewport': 'mobile', 'openOnMobile': false, 'initOpen': '#dossierSummary__title1'}"><h3 id="dossierSummary__title5"
                    class="dossierSummary__title headline--underlined margin-top-50 margin-top-s-0"
                    aria-expanded="false"
                    aria-controls="dossierSummary__item5"
                    data-gtm="chapterAccordion__5"
