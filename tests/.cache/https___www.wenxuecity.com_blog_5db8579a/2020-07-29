<script>
  $(function() {
    $(".gallery .next").click(function() {
      if((2000 - $(".gallery-scroll").scrollLeft()) > 196) {
      	$(".gallery-scroll").scrollLeft($(".gallery-scroll").scrollLeft() + 196);
      }
    });
    
    $(".gallery .pre").click(function() {
      $(".gallery-scroll").scrollLeft($(".gallery-scroll").scrollLeft() - 196);
    });
  });
  </script>
