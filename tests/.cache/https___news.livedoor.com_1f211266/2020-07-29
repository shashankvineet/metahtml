<script type="text/javascript">
$(function() {
  var cookieName = "ldnews-topics-block";
  var save_category = function(index) {
    var d = new Date();
    d.setTime(d.getTime() + (2 * 60 * 60 * 1000)); // 2 hours
    $.cookie(cookieName, index, { path: "/", expires: d });
  }
  $(".secTopMainTopics").find("a").click(function() {
    save_category(0);
  });
  $(".topTopicsCatItem").each(function() {
    var index = $(".topTopicsCatItem").index(this);
    index = index + 1;
    $(this).find("a").click(function(){
      save_category(index);
    });
  });
});
</script>


