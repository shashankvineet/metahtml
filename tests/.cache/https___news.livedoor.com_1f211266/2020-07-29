<script>
(function($) {
  var cookieName = 'livedoor-news-gdpr-agreed';
  $(document).ready(function() {
    if (navigator.userAgent.match(/https?:\/\//)) {
      // UA に http:// or https:// があればスルー
      return;
    }

    var agreed = document.cookie.indexOf(cookieName) >= 0;
    if (agreed) {
      // 同意済み Cookie があればスルー
      return;
    }

    var banner = $('<div>');
    banner.attr('id', 'gdpr-banner');
    var label = $('<p>');
    label.html('\
            当サイトではクッキーを使用しています。当サイトのブラウジングを続けることにより、当社のポリシーを受け入れたものと見なされます。<a href="http://www.livedoor.com/rules/" style="text-decoration: underline;">詳しく見る</a><br>\
    ');
    banner.append(label);

    var closeButton = $('<div>');
    closeButton.html('<img src="/img/v2/common/close.png" style="width: 100%;height: 100%;display: block;"/>');
    closeButton.on('click', function() {
      $('#gdpr-banner').hide();
      document.cookie = cookieName + '="1";expires=Tue, 31-Dec-2037 00:00:00 GMT;path=/;';
    });
    banner.append(closeButton);

    banner.css({
      'position': 'relative',
      'line-height': '1',
      'background': 'rgb(245, 245, 245)',
      'color': '#000',
      'padding': '5px',
      'text-align': 'center',
      'box-shadow': '0 -2px 2px rgba(102, 102, 102, 0.86)',
      //'z-index': '2147483647',
    });
    label.css({
      'display': 'inline-block',
      'text-align': 'left',
      'font-size': '13px',
      'color': '#000',
      'vertical-align': 'middle',
      'line-height': '1.5',
      'margin': '0',
    });
    closeButton.css({
      'display': 'block',
      'position': 'absolute',
      'top': '0',
      'right': '10px',
      'bottom': '0',
      'margin': 'auto',
      'width': '14px',
      'height': '14px',
      'padding': 0,
      //'background-color': '#11ffee00',
      'vertical-align': 'middle',
    });

    var script = document.createElement('script');
    script.type = "text/javascript";

    script.src = "https://countrycheck.livedoor.com/check";

    script.onerror = function() {
      //$('body').append(banner);
      //$('header').prepend(banner);
      $('#container').prepend(banner);
    };
    document.body.appendChild(script);
  });
})(jQuery);
</script>
</body>
</html>

