<script type="text/javascript">
function login_page() 
{ 
buttonRedirect('/STMonitor/qbwnew/login.htm;jsessionid=096C92569160A606B281374E7817ABC2?serverId=172&refer=hy');
} 
function reg_page() 
{ 
buttonRedirect('/STMonitor/qbwnew/register.htm;jsessionid=096C92569160A606B281374E7817ABC2?serverId=172&parentPageId=1596049197844');
} 
function filterSearch() {
	var searchValue = document.getElementById("searchValue").value;
		var value = encodeURIComponent(searchValue);
		var url = '/STMonitor/qbwnew/search.htm;jsessionid=096C92569160A606B281374E7817ABC2?parentPageId=1596049197844&serverId=172&op=quick&value='
				+ value + '';
		buttonRedirect(url);
}
function filterSearchSchool() {
	var searchValue = document.getElementById("searchValue").value;
		var value = encodeURIComponent(searchValue);
		var url = '/STMonitor/qbwnew/searchschool.htm;jsessionid=096C92569160A606B281374E7817ABC2?parentPageId=1596049197844&serverId=172&op=quick&value='
				+ value + '';
		buttonRedirect(url);
}
</script>
 
    
