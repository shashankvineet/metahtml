		function cutObjectTopic(output) {
			var MAX_LENGTH = 6;
			var result = output;
			if (output.length > MAX_LENGTH) {
				var preOutput = output.substring(0, MAX_LENGTH);
				result = preOutput + '...';
			}
			document.write(result);
		}
