      </script> -->

      <script>
         $.ajax({
            type: "GET",
            url: "/include/plugins/jquery.php",
             data: "app=pages,homepage_adinfo&sess=true&func=get_adinfo&ip=&code="+ Math.random(),
             datatype: "json",
             success:function(){
               data= $.parseJSON(res);
               for(v in data)
               {
                  $("#"+v+"_adinfo").html(data[v]);
               }
             }
         });   
      </script>
