    window.regModule("module_imageSubtitle", {"data":{"imageExists":true,"hyperlink":"","hyperlink_target":"","hyperlinkAsString":"","pinterest":"0","id":11790802097,"widthEqualsContent":"1","resizeWidth":"739","resizeHeight":488},"variant":"default","selector":"#cc-m-11790802097","id":11790802097});
