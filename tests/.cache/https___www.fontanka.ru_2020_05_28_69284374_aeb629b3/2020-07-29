      const checkValueCookie = decodeURIComponent(matches[1]);
      if (checkValueCookie) {
        checkNumber = checkValueCookie.toString().slice(-1); 
      }
  }
  if (checkNumber && checkNumber % 2 === 0) {
    console.log('**** Taboola!!!!'); 
     window._taboola = window._taboola || [];
    _taboola.push({
        mode: 'alternating-thumbnails-a',
        container: 'taboola-below-article-thumbnails',
        placement: 'Below Article Thumbnails',
        target_type: 'mix'
    });
  } else {
    if (window.innerWidth > 899) {
      window.Ya.adfoxCode.create({
        ownerId: 320882,
        containerId: 'adfox_159497283897148391',
        params: {
            p1: 'cjwnd',
            p2: 'gqqu'
        }
      });
    } else {
      window.Ya.adfoxCode.create({
        ownerId: 320882,
        containerId: 'adfox_159497283897148391',
        params: {
            p1: 'cjwut',
            p2: 'gqqu'
        }
      });
    }
  }
} catch (error) {
  console.log('*** Read also:', error);
}
</script>
