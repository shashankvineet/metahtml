                        /* portrait tablets */
                        document.write('<span class="td-adspot-title">Advertisement </span><ins class="adsbygoogle" style="display:inline-block;width:435px;height:300px" data-ad-client="ca-pub-1165107090459930" data-ad-slot=""></ins>');
