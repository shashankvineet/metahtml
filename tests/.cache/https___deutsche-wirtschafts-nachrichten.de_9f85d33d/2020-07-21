<script>
if (typeof FIN2 !== 'object'){ var FIN2 = {}; }
if (typeof FIN2.live !== 'object') { FIN2.live = {}; }
FIN2.live.time_mutator = function(s){                                                                                                                                  return s;                                                                                                                                                      };                                                                                                                                                                 FIN2.live.relative_time = {                                                                                                                                            future: "in %s",                                                                                                                                                   past:   "%s",
    s:  "%d Sek",
    m:  "1 Min",
    mm: "%d Min",
    h:  "1 Std",
    hh: "%d Std",
    d:  "1 Tag",
    dd: "%d Tage",
    M:  "1 Mon",
    MM: "%d Mon",
    y:  "1 Jhr",
    yy: "%d Jhr"
};
