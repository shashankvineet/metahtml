<script>
function run_dfp(extra_tgt){
	if( dfp_happened.length > 3 ) { 
		console.log('run_dfp strlen: ', dfp_happened.length);
		return; 
	}

	var ed = 'DWN';
  var section = '';
  var activeEd = false;
  var testEd = ed;
