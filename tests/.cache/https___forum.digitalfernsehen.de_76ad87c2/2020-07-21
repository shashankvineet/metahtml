if ($(window).width() > parseInt('850px', 10) + 160 && true)
{
  $('head').append('<style>#g9340f21b817a{max-width:1360px;margin:0 auto}#contentwrapper{position:relative}#hm_topwrapper,#bodywrapper{width:calc(100% - 160px)}.pageWidth{padding-right:0}#hm_wideskys{display:block !important}</style>');
  $('#hm_rightwrapper').css({
    'position': 'absolute',
    'left': 'calc(100% - 160px)',
    'top': '0',
  });
  if ($(window).height() > 600)
  {
    $('#hm_wideskys').css('position', 'fixed');
    $(window).bind('scroll', function() {
      if ($(window).scrollTop() >= $('#hm_topwrapper').outerHeight())
      {
        $('#hm_wideskys').css('top', '0');
      }
      else
      {
        $('#hm_wideskys').css('top', $('#hm_topwrapper').outerHeight() - $(window).scrollTop());
      }
    });
  }

} else { $('head').append('<style>#hm_wideskys{display:none;}</style>');}
