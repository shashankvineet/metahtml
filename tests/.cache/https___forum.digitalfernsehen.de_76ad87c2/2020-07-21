			SDG.Publisher.registerSlot('topmobile3', 'ax-medium-rectangle-2').load();
			SDG.Publisher.registerSlot('topmobile4', 'ax-medium-rectangle-3').load();
		}

	} else {
		SDG.Publisher.setZone('forum');
		if (SDG.getUtil().getViewportDimensions().width >= parseInt(SDG.Publisher.getConfig().getCommonValue('mobileBreakpoint'))) {
			SDG.Publisher.registerSlot('banner', 'hm_sb').load();
			SDG.Publisher.registerSlot('sky', 'hm_wideskys').load();
