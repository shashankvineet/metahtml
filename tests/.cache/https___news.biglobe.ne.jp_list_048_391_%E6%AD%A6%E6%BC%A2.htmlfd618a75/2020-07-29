<script language="javascript" type="text/javascript">
// ãã¼ã¸ãéããã¨ãã«æå®ããç¯å²åãªãããã¿ã¼åºåãæ¶ãããã«è¨­å®
	var cookieValue = getCookie("yadsOverlay");

	var expires = new Date;
	var new_expires = expires.getTime();
	expires.setTime(new_expires);

	var expires_time = getCookie("yadsOverlay");

	if (cookieValue) {
			document.getElementById('yadsOverlayUnit').style.display='none';
	}
function getCookie(key){
	var cookies = document.cookie.split('; ');
	for (var i = 0; i < cookies.length; i++){
	    var c = cookies[i].split('=');
	    if(c[0] == key){
		    return c[1];
	    }
	}
	return false;
}

</script>

