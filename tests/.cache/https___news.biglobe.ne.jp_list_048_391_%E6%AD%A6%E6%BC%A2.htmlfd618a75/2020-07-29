function OnAnchorCloseClick() {
	document.getElementById('yadsOverlayUnit').style.display='none';
    _gaq.push(['_trackEvent', 'footer_anchor', 'close', 'click']);
	// ã¯ãã­ã¼ã«æ¸ãè¾¼ã
	setAnchorClose();
}

//Cookieã«ä¿å­
function setAnchorClose(){
	//æå¹æéåå¾
	var expires = new Date;
	var domain = 'domain=' + document.domain + ';';
	var time = (expires.getTime() + (3 * 60 * 60 * 24 * 1000 ));

	expires.setTime(time);

	document.cookie = "yadsOverlay=1;" + "expires=" + expires.toUTCString() + ";" + domain + 'path=/;' ;
}
</script>

