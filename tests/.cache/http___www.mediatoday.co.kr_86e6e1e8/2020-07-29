<script>
$(document).ready(function(){
	var $idxMore = $('.index-more')
		, $idxMoreGrp = $idxMore.find('.group')
		, $idxMoreBtn = $idxMore.find('.more-btn');

	$idxMoreBtn.on({
		click: function(){
			var _that = $(this).parent();
			
			// 블럭단위로 노출
			if(_that.children(':first-child').hasClass('active')) {
				_that.children('.active').next().addClass('active');
			} else {
				_that.children(':first-child').addClass('active');
			}
			
			// 모든처리 후 버튼감추기
			if(_that.find('.group:last-of-type').hasClass('active')) {
				$idxMoreBtn.hide();
			}
			return false;
		}
	});
});
</script>

