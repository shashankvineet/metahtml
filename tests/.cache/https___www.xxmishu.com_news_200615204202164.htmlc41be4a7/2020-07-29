  var key = "b4e897b0a954baea80f91c56bbe8bb52" ;
var nid = 438335 ;

function dofavorite(){
	$.ajax({
		url:"/home/news/dofavorite.html",
		data:{key:key,id:nid},
		type:'post',
		dataType:"json",
		success:function(data){
			if(data.code == 1){
				layer.msg(data.msg,{icon:6});
				$("#bookmark_button i").addClass("on");
				$("#bookmark_button span").html("已收藏");
			}else{
				layer.msg(data.msg,{icon:5});
			}
		}
	});
}
$(".mytags").click(function(){
		tag = $(this).text();
		window.location.href = '/tag/' + tag + '.html' ;
	});
  $("#news_hits").click(function(){
		$.ajax({
             type: "GET",
             url: "/home/Ajax/update_news_hits/id/200615204202164",
             data: {t: Math.random()},
             dataType: "text",
             success: function(result){
			 //alert(result);
			 //layer.msg(result);
			    if (result)
				{
				    $("#news_hits").html(result);
				}
                
             },
			 error: function(){

             },
			 
		});
	});
  
