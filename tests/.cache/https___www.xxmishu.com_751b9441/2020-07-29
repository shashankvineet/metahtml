<script>
$(document).ready(function(){
    
    $("#wxscan").click(function(){
	    //alert("wxscan");
		$("#ul_wxscan").css("display",'block');
		$("#ul_ulogin").css("display",'none');
		polling_login();
	});
	$("#ulogin").click(function(){
	    //alert("wxscan");
		$("#ul_wxscan").css("display",'none');
		$("#ul_ulogin").css("display",'block');
		expire_qr();
	});
});


	//请求登录二维码
var scene_id=0;
function polling_login() {
    $.ajax({
        url:"/api/Wechat/ticket_img/",    //请求的url地址
        dataType:"json",   //返回格式为json
        async:true,//请求是否异步，默认为异步，这也是ajax重要特性
        data:{"t":randomNum(10000,99999)},    //参数值
        type:"POST",   //请求方式
        beforeSend:function(){
            //请求前的处理
        },
        success:function(req){
            //请求成功时处理
            var json = JSON.parse(req);
			if (json.status=="1"){
			    $(".ticket_img").attr("src",json.ticket_img);//alert(json.ticket_img);
				scene_id=json.scene_id;
				check_login(scene_id);
				$(".top-tip-ghostwriting").css("display","none");
				//clearTimeout(stop_sec);
				stop_sec=setTimeout("expire_qr()",300000);//开始计时二维码过期时间 60s
				//alert("scene_id:"+scene_id);
			}
        },
        complete:function(data){
            //请求完成的处理
        },
        error:function(){
            //请求出错处理
        }
    });
}	
function check_login(scene_id) {
    $.post("/api/Wechat/check_scan/t/"+randomNum(10000,99999), {scene_id: scene_id}, function(data) {
        if (data.status == 1) {
		    //alert("scene_id:"+scene_id+" status:"+data.status);
            //alert("欢迎您"+data.nickname+" 登录成功！");
			layer.msg("欢迎您"+data.nickname+" 登录成功！", {icon: 1});
            location.reload();
        } else {
		    //alert("scene_id:"+scene_id);
            check_qr=setTimeout("check_login("+scene_id+")", 2000);
        }
    }, "json");
}
function expire_qr()
{
	$(".top-tip-ghostwriting").css("display","block");
	
	clearTimeout(check_qr);
	clearTimeout(stop_sec);
}
$(".qr-load").click(function(){
	polling_login();
	$(".top-tip-ghostwriting").css("display","none");
});
//生成从[min,max]的随机数
//Math.floor(Math.random()*minNum+1); 可以用替换为
//parseInt(Math.random()*(max-min+1)+min,10);
function randomNum(min,max){ 
    switch(arguments.length){ 
        case 1: 
            return Math.floor(Math.random()*minNum+1); 
        break; 
        case 2: 
            return Math.floor(Math.random()*(max-min+1)+min); 
        break; 
            default: 
                return 0; 
            break; 
    } 
}


