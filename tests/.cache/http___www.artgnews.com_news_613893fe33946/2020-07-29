<script type="text/javascript">

$(".critical_btn").click(function() {
  $(this).toggleClass('btn-default btn-info ');
});


   //탭 슬라이드형
       function sideDetailTab() {
	   $('#swrap_detail_tab .list_detail_tab .tab_detail').click(function() {
	
		   var tabListIndex = $(this).parent().index();
		   var detailTabCont = $('#sdetail_tab .detail_tab_cont');
		   $('#swrap_detail_tab .list_detail_tab li').removeClass('on');
		   $(this).parent().addClass('on');
		   detailTabCont.removeClass('show').eq(tabListIndex).addClass('show');
	
		   return false;

	   });
    }
	

$(document).ready(function(){


	// 배너
      	uiCommon.setAdRandomEvent(".side_banner_1", ".list_ad li", ".btn_page .prev", ".btn_page .next");
      
      	uiCommon.setAdRandomEvent(".side_banner_2", ".list_ad li", ".btn_page .prev", ".btn_page .next");
      
      	uiCommon.setAdRandomEvent(".side_banner_3", ".list_ad li", ".btn_page .prev", ".btn_page .next");
      

    	sideDetailTab();
    


});
</script>   
   
