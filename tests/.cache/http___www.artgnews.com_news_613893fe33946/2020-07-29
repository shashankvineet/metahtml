        function googleTranslateElementInit() {
            $.when(
                new google.translate.TranslateElement({pageLanguage: 'ko', includedLanguages: 'ko,en,ja,zh-CN',
                    layout: google.translate.TranslateElement.InlineLayout.SIMPLE, autoDisplay: false, multilanguagePage: true}, 'google_translate_element')
            ).done(function(){
                //$('.translation-icons a.us').click(); 
            });
        }
