	<script type="text/javascript">
		function spoilermod_toggle($this) {
			$this = $($this);
			var $_desc = $this.children('span.spoiler_desc');
			var $_div = $this.siblings('div.spoiler_div');
				
			$_desc.html($_div.is(':visible') ? ' (Anzeigen)' : ' (Verstecken)');
			$_div.toggle();

			return false;
		}
	</script>
