		function openReference(id) {
			if ($('#reference-button-open-'+id).is(":visible"))
				$('#reference-button-open-'+id).hide();
			if ($('#reference-button-close-'+id).is(":visible"))
				$('#reference-button-close-'+id).show();
			$('#reference-box-'+id).show();
		}
		function closeReference(id) {
			$('#reference-box-'+id).hide();
			if ($('#reference-button-close-'+id).is(":visible"))
				$('#reference-button-close-'+id).hide();
			if ($('#reference-button-open-'+id).is(":visible"))
				$('#reference-button-open-'+id).show();
		}
		function switchReference(id, clicked) {
			if ($('#reference-box-'+id).is(":visible"))
				closeReference(id);
			else openReference(id);
			$(clicked).blur();
		}
	</script>

