<script type="text/javascript">
$(function() {
  var $list = $('.rankingBlockNav li'),
      $item = $('.rankingBlockItem'),
      selectedClass = 'selected';
  $list.find('a').on('click',function(){
    var num = $list.index($(this).parent('li'));
    $list.removeClass(selectedClass);
    $list.eq(num).addClass(selectedClass);
    $item.removeClass(selectedClass);
    $item.eq(num).addClass(selectedClass);
  })
});
</script>

        
