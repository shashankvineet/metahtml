<script type="text/javascript" charset="utf-8" src="https://cdn.weeklytoday.com/script/masonry.pkgd.min.js"></script>
<script type="text/javascript">

	var jQuerytileContainer = null;
	var tailCnt = 7;				// 한번에 노출될 갯수를 설정한다.
	var cur = 0;						// 현재 노출된 갯수설정을 위한 커서 ( 변경하지 마세요 )
	jQuery(function(){

		// 타일생성
		function setTail(){
			$tileContainer = jQuery('#auto-16');
			$tileContainer.masonry({
			  itemSelector: '.targets-16',
			  gutter:0,
			  isOriginLeft:true,
			  isOriginTop:true
			});

		}

		// 아이템 생성
		function setItem(event){
			var cutLimit = tailCnt * (cur+1);
			var itemCnt = jQuery(".targets-16").length;


			jQuery(".targets-16:lt("+cutLimit+")").css('display','block');
			setTail();
			cur++; 
			if(event.pageY !== undefined) 	jQuery(window).scrollTop(event.pageY-80);


			if(cutLimit >= itemCnt){

				// 더보기 클릭
				jQuery("#more-16").unbind( "click" );
				jQuery("#more-16").click(function(){
					jQuery(".targets-16").css('display','none');
					jQuery(".targets-16:lt("+tailCnt+")").css('display','block');
					setTail();
					cur=1; 
					
					jQuery("#more-16").unbind( "click" );
					jQuery("#more-16").bind({
						click:function(event){
							setItem(event);
						}
					});

					jQuery("#more-16").attr("href","/news/articleList.html?view_type=sm");

				});
			}
			
		}

		// 이벤트 바인드
		jQuery(document).ready(function(event) {

			// init item
			setItem(event);

			// 더보기 클릭
			jQuery("#more-16").bind({
				click:function(event){
					setItem(event);
				}
			});


		});
	});

</script>

</section>
