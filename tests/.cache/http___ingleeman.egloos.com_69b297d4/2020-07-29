	function sendGnbClick(cm) {
		var obj = event.currentTarget || event.srcElement;
		var estatData = {
			"cm":cm,
			"pos":"egloos_gnb",
			"url": document.location.href,
			"referrer":document.referrer
		};
		if(cm != "more") {
			estatData['outurl'] = obj.href;
		}
		ESTatTracker.push(["@Click", estatData]);    
	}
