<script>
function getCanonical() {
var metas = document.getElementsByTagName('link');
for (i=0; i<metas.length; i++) {
if (metas[i].getAttribute("rel") == "canonical") {
return metas[i].getAttribute("href");
}
}
return "n/a";
}

