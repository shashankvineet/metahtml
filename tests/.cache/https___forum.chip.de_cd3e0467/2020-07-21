        always: ["\/\/a.bf-ad.net\/adengine\/chip\/adengine.js","\/\/content.chip.de\/cac\/adconfig.js","\/\/content.chip.de\/cac\/adgoal.js"],
        desktop: ["\/\/content.chip.de\/cac\/vibrant.js"],
        tablet: [],
        smartphone: []
    };

    for (var i = 0; i < cac_ext_scripts.always.length; i++) {
        getExternalJS(cac_ext_scripts.always[i]);
    }

