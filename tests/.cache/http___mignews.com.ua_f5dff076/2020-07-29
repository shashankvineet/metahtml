<script type="text/javascript">
    (function () {
        $("body").click(function (e) {
            var ib = $(e.target).closest("[data-type=informer]");
            if (ib && ib.length > 0) {
                var a = $(e.target).closest("a");
                if (a.length == 0) {
                    a = $(e.target).children("a");
                }
                var c = ib.attr("data-category");
                if (!c) {
                    c = "Informers";
                }
                var s = {
                    'eventCategory': c,
                    'eventAction': ib.attr("data-target") + '-click',
                    'eventLabel': a.length > 0 ? a.attr("href") : "",
                    'nonInteraction': true
                };
                ga('send', 'event', s);
            }
            return true;
        });
    })();
</script>
