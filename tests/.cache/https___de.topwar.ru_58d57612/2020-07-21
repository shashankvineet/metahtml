<script>
		var dle_device = 'desktop;'
		
		
		function search_open() {
			document.getElementById("page_body").classList.toggle("qsearch_open");
		}
		
		
		window.onscroll = function() {
			upper();
			
		};
		function upper() {
			if (document.body.scrollTop > 200 || document.documentElement.scrollTop > 200) {
				document.getElementById("upper_left").className = "active";
			} else {
				document.getElementById("upper_left").className = "";
			}
		};
		

		$(function() {
			$('.scrollbar').scrollbar();

			$('#darkMode').on('click', function(e){
				e.preventDefault;
				$.post('/custom/change.theme.php', {'night_mode': $(this).val()}).done(function() { ShowLoading(""); location.reload() })
			})

			

			$('.dropdown-form').click(function(e) {
				e.stopPropagation();
			});

			
            
				auction('finobzor');
				auction('politobzor');
            

			
				
				$('.upper, #upper_left').click(function(){
					$('html, body').animate({scrollTop:0}, 'slow');
					return false;
				});

				
				$('.soc_login a').on('click',function(){
				   var href = $(this).attr('href');
				   var width  = 820;
				   var height = 420;
				   var left   = (screen.width  - width)/2;
				   var top   = (screen.height - height)/2-100;

