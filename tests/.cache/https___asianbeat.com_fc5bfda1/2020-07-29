<script>
$(document).ready(function(){
	//コンテンツの高さを取得
	var cHeight = $('#index article').height();

	//リサイズ
	auto_resize();

	$(window).on('load resize',function(){
		auto_resize();
	});

	function auto_resize(){
		//画面高さを取得
		var wHeight = $(window).height();

		if(cHeight > wHeight){
			$('#index article').css('height',cHeight);
		} else {
			$('#index article').css('height',wHeight);
		}
	}
});
</script>
