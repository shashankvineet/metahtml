<script type="text/javascript">
(function($){
    function cutString(lmn) {
        $(lmn).each(function(){
            // name
            var $targetN = $(this).find('a');
            var name = $targetN.text();
            var lengthN = name.length;
            while ((name.length > 0) && ($targetN.height() > 36)) { // font size 1 line
                name = name.substr(0, name.length - 1);
                $targetN.text(name);
            }
            if (lengthN != name.length) {
                name = name.substr(0, name.length - 2) + '…';
            }
            $targetN.text(name);
        });
    }
    $(document).ready(function() {
        var lmn = $('div.dAccessRanking .subStrText');
        if (lmn != null) cutString(lmn);
    });
})(jQuery);
</script>
