    <script type="text/javascript">
  $(document).ready(function () {
    window.setTimeout(function () {

      var ak_stat = "wdr";
      var ak_mobi = "mobwdr";

      function isStringDetected(value) {
        var regexp = new RegExp(value, "gi");
        return (regexp.test(navigator.appVersion) || regexp.test(navigator.userAgent)) ? true : false;
      }

      function isTablet() {
        if (isStringDetected("iPad|Windows NT|Kindle|PlayBook")
            || (isStringDetected("Android") && !isStringDetected("Mobile"))) {
          return true;
        }
        return false;
      }

      $.ajaxSetup({
        cache: true
      });
      $.getScript("//script.ioam.de/iam.js").done(
          function () {
            var isMobil = typeof orientation != 'undefined' ?  true : false;
            if (isTablet()) {
              isMobil = false;
            }
            if (!isMobil) {
              var iam_data = {
                "st": ak_stat,
                "cp": "Seite/Stationär"
              };
            } else {
              var iam_data = {
                "st": ak_mobi,
                "cp": "Seite/Mobil"
              };
            }
            iom.c(iam_data, 1);
          });
    }, 1);
  });
</script>

<script type="text/javascript">
  var contentGroupBase = "WDR,Kultur,Digit".split(/,/);
  var contentGroups = contentGroupBase.concat("Startseite".split(/,/));

  window.digit.webtrekk = {};
  window.digit.webtrekk.contentGroupBase = contentGroupBase;
  window.digit.webtrekk.contentGroup = contentGroups;

  digit.analytics.activate('webtrekk');
  digit.analytics.webtrekk.trackWebtrekk();
</script>


