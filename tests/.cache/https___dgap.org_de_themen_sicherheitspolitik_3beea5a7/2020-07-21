  <article role="article" about="https://internationalepolitik.de/de/deutschlands-bedrohungslage" class="node node--type-ip-article node--view-mode-teaser">
              
              <div class="blazy field field--name-field-teaser-picture field--type-entity-reference field--label-hidden field__item" data-blazy="" data-content-gallery>



                      <a href="https://internationalepolitik.de/de/deutschlands-bedrohungslage">      <div class="media media--blazy media--loading media--switch media--switch--content media--image">
      
<img height="248" width="436" class="b-lazy media__image media__element" data-src="/sites/default/files/styles/dgap_teaser_small/public/IP_05-2019_Forsa.jpg?itok=Eut1Tr1R" alt="Bild: Grafik " src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7" typeof="foaf:Image" />

        
      
